[user]
  name = Nesterov Roman
  email = nester@bk.ru
[alias]
  co = checkout
  br = branch
  st = status
  cm = commit -m
  cam = commit --amend
  ds = diff --staged
  fix = commit --fixup
  squash = commit --squash
  pr = pull --rebase
  rc = rebase --continue
  ri = rebase --interactive --autosquash
  ra = rebase --abort
  rs = rebase --skip
  cp = cherry-pick
  sub = submodule update --init
  hi = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  show-files = show --pretty="format:" --name-only
  lt = describe --abbrev=0 --tags
  bl = !sh -c 'git blame $0 | grep -A 1 -B 1 00000000'
  today = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short --author Roman --since="6am" develop
[rerere]
  enabled = 0
[push]
  default = simple
[color]
  ui = false
