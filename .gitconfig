[user]
  name = Nesterov Roman
  email = nester@bk.ru
[alias]
  co = checkout
  br = branch
  st = status
  cm = commit -m
  cam = commit --amend
  ds = diff --staged
  fix = commit --fixup
  squash = commit --squash
  pr = pull --rebase
  rc = rebase --continue
  ri = rebase --interactive --autosquash
  ra = rebase --abort
  rs = rebase --skip
  cp = cherry-pick
  sub = submodule update --init
  hi = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short --abbrev=8
  rlog = log --no-merges --pretty='format:%cs %s'
  sf = show --pretty="format:" --name-only
  lt = describe --abbrev=0 --tags
  show-files = log --name-only 
  bl = !sh -c 'git blame $0 | grep -A 1 -B 1 00000000'
  mo = !vim `git ls-files -m`
  #moc = !vim `git show --pretty=format: --name-only $1`
  moc = !sh -c 'vim `git show --pretty=format: --name-only $1`' -
  today = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short --author Roman --since="6am" develop
  parent = "!git show-branch | grep '*' | grep -v \"$(git rev-parse --abbrev-ref HEAD)\" | head -n1 | sed 's/.*\\[\\(.*\\)\\].*/\\1/' | sed 's/[\\^~].*//' #"
  commiters = shortlog --summary --numbered --email
[rerere]
  enabled = 0
[push]
  default = simple
[color]
  ui = false
