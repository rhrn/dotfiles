[user]
	name = rhrn
	email = maylog@gmail.com
[core]
	autocrlf = false
	safecrlf = true
[alias]
  co = checkout
  br = branch
  st = status
  cm = commit -m
  cam = commit --amend
  ds = diff --staged
  fix = commit --fixup
  squash = commit --squash
  pr = pull --rebase
  rc = rebase --continue
  ri = rebase --interactive --autosquash
  sub = submodule update --init
  hi = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  bl = !sh -c 'git blame $0 | grep -A 1 -B 1 00000000'
